package Aplicacao;

import entidade.Consumo;

public class AppConsumo {
    public static void main(String[] args)  throws Exception {

        Consumo consumo = new Consumo(35);
        System.out.println(consumo.toString());


        Consumo consumo1 = new Consumo('f', 25);
        System.out.println(consumo1.toString());


        Consumo consumo2 = new Consumo('a', 23);
        System.out.println(consumo2.toString());


        Consumo consumo3 = new Consumo('a', 35);
        System.out.println(consumo3.toString()); 
        
    }
}
