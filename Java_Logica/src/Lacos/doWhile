import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
	    int soma=0;
	    int i = 0;
    do{
        System.out.printf("Informe um numero");
        int numero = input.nextInt();
        soma += numero;
        i+=1;
    }while(i<7);
    System.out.println(soma);
	}
}
