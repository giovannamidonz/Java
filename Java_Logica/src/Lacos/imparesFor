package PraticaLogica;
import java.util.Scanner;
public class imparesFor {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int x ;
        do{
            System.out.println("Digite um Valor: ");
            x = input.nextInt();

            for(int i = 0; i <= x ; i++ ){
                if((i%2)!=0){
                    System.out.println(i);
                }
            }
        }while(1 >= x && x >= 1000);



    }
}
