package PraticaLogica;
import java.util.Scanner;
public class inOut {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int x = 0;
        int N;
        int out = 0;
        int in = 0;
        System.out.println("informe o valor de N vezes que vocÃª quer adicionar : ");
        N = input.nextInt();
        for( int i = 0 ; i < N; i++) {
            System.out.println("informe o valor que voce deseja adicionar : ");
            x = input.nextInt();
            if (x >= 10 && x <= 20) {
                in++;
            } else {
                out++;
            }
        }
            System.out.println(in + " in");

            System.out.println(out + " out");


    }
}
